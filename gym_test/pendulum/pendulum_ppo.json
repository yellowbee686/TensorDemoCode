{
  "type": "ppo_agent",

  "update_mode": {
      "unit": "episodes",
      "batch_size": 5,
      "frequency": 5
  },
  "memory": {
      "type": "latest",
      "include_next_states": false,
      "capacity": 50000
  },

  "baseline_mode": "states",
  "baseline": {
      "type": "mlp",
      "sizes": [32, 32]
  },
  "baseline_optimizer": {
      "type": "multi_step",
      "optimizer": {
          "type": "adam",
          "learning_rate": 2e-4
      },
      "num_steps": 10
  },

  "discount": 0.99,
  "entropy_regularization": 1e-3,
  "gae_lambda": 0.97,
  "likelihood_ratio_clipping":0.2,

  "step_optimizer": {
      "type": "adam",
      "learning_rate": 1e-4
  },
  "subsampling_fraction": 0.1,
  "optimization_steps": 10
}